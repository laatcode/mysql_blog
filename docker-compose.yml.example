version: '3.8'

services:
  db:
    image: mysql
    restart: always
    command: --default-authentication-plugin=mysql_native_password
    environment:
      - MYSQL_ROOT_PASSWORD=YOUR_ROOT_PASSWORD
      - MYSQL_DATABASE=YOUR_DB_NAME
      - MYSQL_USER=YOUR_DB_USER
      - MYSQL_PASSWORD=YOUR_DB_PASS
    ports:
      - 3306:3306